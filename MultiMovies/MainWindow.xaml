<Window x:Class="MultiMovies.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiMovies"
        xmlns:conv="clr-namespace:MultiMovies.Lib.Converters"
        mc:Ignorable="d"
        xmlns:v="clr-namespace:MultiMovies.Views"
        xmlns:vm="clr-namespace:MultiMovies.ViewModels"
        Title="Home Page"
        Height="800" Width="1000"
        MinHeight="600" MinWidth="1000"
        ResizeMode="CanResize">

    <!--<v:M3U8Player />-->

    <ScrollViewer VerticalScrollBarVisibility="Auto"
                  HorizontalScrollBarVisibility="Disabled">
        <Grid Background="{DynamicResource BackgroundBase100}">
            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
        </Grid>
    </ScrollViewer>
</Window>
